<Application
    x:Class="toyota.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:toyota"
    xmlns:localData="using:toyota.Data">

    <Application.Resources>

        <!-- Application-specific resources -->

        <x:String x:Key="AppName">Toyota Cars</x:String>
        <ImageBrush x:Key="gasBrush" ImageSource="/Assets/bg.png" Opacity="100" Stretch="UniformToFill" />
    </Application.Resources>
</Application>

